<a href="/">Home</a>
<h1>Show a Set</h1>
<h3>Setname</h3>
<p><%= @setname %></p>
<h3>Set of videos:</h3>
<p><%= session["sets"][@setname]["vidnums"] %></p>

<form action="/sets/<%= @setname %>" method="post">
