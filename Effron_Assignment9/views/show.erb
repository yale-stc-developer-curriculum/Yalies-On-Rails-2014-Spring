<h1>Show a Set</h1>
<h3>Setname</h3>
<p><%= @setname %></p>
<h3>Set of videos:</h3>
<% @videos.each do |video| %>
	<p><%= video %> </p>
<% end %>

<form action="/sets/<%= @setname %>" method="post">
    <input name="_method" type="hidden" value="delete" />
    <input type="submit" value="Delete">
</form>
