<h1> Edit an existing set </h1>
<!-- Action is the URL to which it is submitted, method is method -->

<h2> Form to edit an existing set </h2>
<form action="/sets/<%=@setname%>" method="put">
<!--	<label>Set Name</label>
	<input type="text" name = "setname"> -->
	<label> List of videos you would like to add </label>
	<textarea name = "videostoadd" placeholder = "ADD"></textarea>
	<label> List of videos you would like to delete </label>
	<textarea name = "videostodelete" placeholder = "DELETE"></textarea>
	<input type="submit" name="submit" value="Submit Form">
</form>
